{
  "name": "group2devops",
  "version": "1.0.0",
  "description": "``` git checkout https://github.com/Group2DevOpsOrg/Group2DevOps.git ```",
  "main": "index.mjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-frontend": "vite build --sourcemap=true --minify false",
    "dev": "npm run build-frontend && node index.mjs",
    "backend-test": "c8 --config .nycrc-backend mocha",
    "frontend-instrument": "npm run build-frontend && rm -rf instrumented && cp -R dist instrumented && nyc instrument dist instrumented",
    "frontend-test": "npm run frontend-instrument && rm -rf .nyc_output && nyc --nycrc-path .nycrc-frontend cypress run",
    "frontend-cypress-gui": "npm run frontend-instrument && rm -rf .nyc_output && nyc --nycrc-path .nycrc-frontend cypress open"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.3",
    "c8": "^10.1.2",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "class-transformer": "^0.1.0-beta.10",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "cypress": "^13.16.0",
    "express": "^4.21.1",
    "fs": "^0.0.1-security",
    "http-status-codes": "^2.3.0",
    "nodejs-snowflake": "^2.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/register": "^7.25.9",
    "@cypress/code-coverage": "^3.13.8",
    "glob": "^11.0.0",
    "istanbul-lib-coverage": "^3.2.2",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "vite": "^5.4.10",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-istanbul": "^6.0.2"
  }
}
